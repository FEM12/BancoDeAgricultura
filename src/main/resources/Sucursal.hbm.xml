<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.entities.Sucursal" table="sucursal" schema="bancodeagricultura">
        <id name="id">
            <column name="ID" sql-type="int"/>
        </id>
        <property name="direccion">
            <column name="Direccion" sql-type="text"/>
        </property>
        <property name="telefonoFijo">
            <column name="TelefonoFijo" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="correoElectronico">
            <column name="CorreoElectronico" sql-type="text" not-null="true"/>
        </property>
        <property name="idGerente">
            <column name="ID_Gerente" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>