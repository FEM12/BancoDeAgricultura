<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.entities.Empresa" table="empresa" schema="bancodeagricultura">
        <id name="id">
            <column name="ID" sql-type="int"/>
        </id>
        <property name="nombre">
            <column name="Nombre" sql-type="varchar(250)" length="250"/>
        </property>
        <property name="nit">
            <column name="NIT" sql-type="varchar(17)" length="17"/>
        </property>
        <property name="telefono">
            <column name="Telefono" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="correoElectronico">
            <column name="CorreoElectronico" sql-type="text"/>
        </property>
        <property name="direccion">
            <column name="Direccion" sql-type="text"/>
        </property>
        <property name="idCliente">
            <column name="ID_Cliente" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>