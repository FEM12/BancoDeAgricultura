<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.entities.Dependiente" table="dependiente" schema="bancodeagricultura">
        <id name="id">
            <column name="ID" sql-type="int"/>
        </id>
        <property name="nombres">
            <column name="Nombres" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="apellidos">
            <column name="Apellidos" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="genero">
            <column name="Genero" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="dui">
            <column name="DUI" sql-type="char(10)" length="10"/>
        </property>
        <property name="fechaDeNacimiento">
            <column name="FechaDeNacimiento" sql-type="date"/>
        </property>
        <property name="estadoCivil">
            <column name="EstadoCivil" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="negocio">
            <column name="Negocio" sql-type="text"/>
        </property>
        <property name="ocupacion">
            <column name="Ocupacion" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="ingresoMensual">
            <column name="IngresoMensual" sql-type="decimal(6,2)" precision="6" scale="2"/>
        </property>
        <property name="telefonoMovil">
            <column name="TelefonoMovil" sql-type="char(9)" length="9"/>
        </property>
        <property name="correoElectronico">
            <column name="CorreoElectronico" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="direccion">
            <column name="Direccion" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>