<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.entities.Credencialcliente" table="credencialcliente" schema="bancodeagricultura">
        <id name="id">
            <column name="ID" sql-type="int"/>
        </id>
        <property name="usuario">
            <column name="Usuario" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="contrasena">
            <column name="Contrasena" sql-type="varchar(70)" length="70"/>
        </property>
        <property name="idCliente">
            <column name="ID_Cliente" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>