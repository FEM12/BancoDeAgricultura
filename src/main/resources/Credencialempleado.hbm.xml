<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.entities.Credencialempleado" table="credencialempleado" schema="bancodeagricultura">
        <id name="id">
            <column name="ID" sql-type="int"/>
        </id>
        <property name="usuario">
            <column name="Usuario" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="contrasena">
            <column name="Contrasena" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="idEmpleado">
            <column name="ID_Empleado" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>