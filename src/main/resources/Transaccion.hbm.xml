<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.entities.Transaccion" table="transaccion" schema="bancodeagricultura">
        <id name="id">
            <column name="ID" sql-type="int"/>
        </id>
        <property name="idCuenta">
            <column name="ID_Cuenta" sql-type="int"/>
        </property>
        <property name="tipoDeTransaccion">
            <column name="TipoDeTransaccion" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="monto">
            <column name="Monto" sql-type="decimal(20,2)" precision="20" scale="2"/>
        </property>
        <property name="fechaHora">
            <column name="FechaHora" sql-type="datetime"/>
        </property>
        <property name="cuentaDeDestino">
            <column name="CuentaDeDestino" sql-type="char(20)" length="20" not-null="true"/>
        </property>
        <property name="idEmpleado">
            <column name="ID_Empleado" sql-type="int" not-null="true"/>
        </property>
        <property name="comisionEmpleado">
            <column name="ComisionEmpleado" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>